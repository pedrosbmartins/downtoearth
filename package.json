{
  "name": "downtoearth-vanilla",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "concurrently \"yarn:tsc:watch\" \"yarn:browserify:watch\" \"live-server --watch=build/bundle.js\"",
    "tsc:watch": "tsc -w",
    "browserify": "browserify -t browserify-css build/src/main.js -o build/bundle.js",
    "browserify:watch": "watchify -t browserify-css build/src/main.js -o build/bundle.js -v",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "devDependencies": {
    "@types/jest": "^29.2.6",
    "@types/mapbox-gl": "^2.7.10",
    "@types/mapbox__mapbox-gl-geocoder": "^4.7.3",
    "browserify": "^17.0.0",
    "browserify-css": "^0.15.0",
    "concurrently": "^7.6.0",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "ts-jest": "^29.0.5",
    "tsc": "^2.0.4",
    "typescript": "^4.9.4",
    "watchify": "^4.0.0"
  },
  "dependencies": {
    "@mapbox/mapbox-gl-geocoder": "^5.0.1",
    "@turf/turf": "^6.5.0",
    "mapbox-gl": "^2.11.1"
  }
}
