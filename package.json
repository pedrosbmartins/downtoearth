{
  "name": "downtoearth-vanilla",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "start": "concurrently \"yarn:build:tsc:watch\" \"yarn:build:browserify:watch\" \"yarn:serve\"",
    "build": "tsc && yarn build:browserify",
    "build:tsc:watch": "tsc -w",
    "build:browserify": "browserify -t browserify-css build/src/main.js -o build/bundle.js",
    "build:browserify:watch": "watchify -t browserify-css build/src/main.js -o build/bundle.js -v",
    "serve": "live-server --watch=build/bundle.js,index.html,assets",
    "test": "jest",
    "test:watch": "jest --watch",
    "schema": "bin/setup-schema"
  },
  "devDependencies": {
    "@types/jest": "^29.2.6",
    "browserify": "^17.0.0",
    "browserify-css": "^0.15.0",
    "concurrently": "^7.6.0",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "live-server": "1.2.1",
    "ts-jest": "^29.0.5",
    "tsc": "^2.0.4",
    "typescript": "^4.9.4",
    "watchify": "^4.0.0"
  },
  "dependencies": {
    "@maplibre/maplibre-gl-geocoder": "^1.5.0",
    "@turf/turf": "^6.5.0",
    "maplibre-gl": "^3.5.2",
    "nanoid": "3"
  }
}
